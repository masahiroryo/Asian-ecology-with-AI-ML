@article{ WOS:000398175200005,
Author = {Young, Nicholas E. and Anderson, Ryan S. and Chignell, Stephen M. and
   Vorster, Anthony G. and Lawrence, Rick and Evangelista, Paul H.},
Title = {A survival guide to Landsat preprocessing},
Journal = {ECOLOGY},
Year = {2017},
Volume = {98},
Number = {4},
Pages = {920-932},
Month = {APR},
Abstract = {Landsat data are increasingly used for ecological monitoring and
   research. These data often require preprocessing prior to analysis to
   account for sensor, solar, atmospheric, and topographic effects.
   However, ecologists using these data are faced with a literature
   containing inconsistent terminology, outdated methods, and a vast number
   of approaches with contradictory recommendations. These issues can, at
   best, make determining the correct preprocessing workflow a difficult
   and time-consuming task and, at worst, lead to erroneous results. We
   address these problems by providing a concise overview of the Landsat
   missions and sensors and by clarifying frequently conflated terms and
   methods. Preprocessing steps commonly applied to Landsat data are
   differentiated and explained, including georeferencing and
   co-registration, conversion to radiance, solar correction, atmospheric
   correction, topographic correction, and relative correction. We then
   synthesize this information by presenting workflows and a decision tree
   for determining the appropriate level of imagery preprocessing given an
   ecological research question, while emphasizing the need to tailor each
