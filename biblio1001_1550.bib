@article{ WOS:000690690100001,
Author = {Dujon, Antoine M. and Vittecoq, Marion and Bramwell, Georgina and
   Thomas, Frederic and Ujvari, Beata},
Title = {Machine learning is a powerful tool to study the effect of cancer on
   species and ecosystems},
Journal = {METHODS IN ECOLOGY AND EVOLUTION},
Year = {2021},
Volume = {12},
Number = {12},
