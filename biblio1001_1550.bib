@article{ WOS:000441753000018,
Author = {Pei, Jingwen and Chu, Chong and Li, Xin and Lu, Bin and Wu, Yufeng},
Title = {CLADES: A classification-based machine learning method for species
   delimitation from population genetic data},
Journal = {MOLECULAR ECOLOGY RESOURCES},
Year = {2018},
Volume = {18},
Number = {5},
Pages = {1144-1156},
Month = {SEP},
Abstract = {Species are considered to be the basic unit of ecological and
   evolutionary studies. As multilocus genomic data are increasingly
   available, there have been considerable interests in the use of DNA
   sequence data to delimit species. In this study, we show that machine
   learning can be used for species delimitation. Our method treats the
   species delimitation problem as a classification problem for identifying
   the category of a new observation on the basis of training data.
   Extensive simulation is first conducted over a broad range of
   evolutionary parameters for training purposes. Each pair of known
   populations is combined to form training samples with a label of same
   species or different species. We use support vector machine (SVM) to
   train a classifier using a set of summary statistics computed from
   training samples as features. The trained classifier can classify a test
   sample to two outcomes: same species or different species. Given
   multilocus genomic data of multiple related organisms or populations,
   our method (called CLADES) performs species delimitation by first
   classifying pairs of populations. CLADES then delimits species by
   maximizing the likelihood of species assignment for multiple
   populations. CLADES is evaluated through extensive simulation and also
   tested on real genetic data. We show that CLADES is both accurate and
