@article{ WOS:000254227100019,
Author = {Pivard, Sandrine and Demsar, Damjan and Lecomte, Jane and Debeljak,
   Marko and Dzeroski, Saso},
Title = {Characterizing the presence of oilseed rape feral populations on field
   margins using machine learning},
