@article{ WOS:000634272100001,
Author = {Gain, Clement and Francois, Olivier},
Title = {LEA 3: Factor models in population genetics and ecological genomics with
   R},
Journal = {MOLECULAR ECOLOGY RESOURCES},
Year = {2021},
Volume = {21},
